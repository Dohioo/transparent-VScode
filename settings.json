{
  "explorer.confirmDelete": false,
  "editor.fontSize": 16,
  "terminal.integrated.fontFamily": "monospace",
  "terminal.integrated.tabs.description": "{cwdFolder}",
  "terminal.integrated.tabs.title": "",
