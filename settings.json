        "settings": {
          "foreground": "#b6b1b1"
        }
      },
      {
        "name": "Variable",
        "scope": ["variable", "entity.name.variable"],
        "settings": {
          "foreground": "#ff7edb"
        }
      },
      {
        "name": "Language variable",
        "scope": "variable.language",
        "settings": {
          "foreground": "#fe4450",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Parameter",
